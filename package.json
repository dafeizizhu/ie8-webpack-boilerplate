{
  "name": "ie8-webpack-boilerplate",
  "version": "0.0.3",
  "description": "Webpack boilerplate for IE8",
  "keywords": [
    "webpack",
    "ie8",
    "boilerplate"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dafeizizhu/ie8-webpack-boilerplate"
  },
  "scripts": {
    "dev": "webpack-dev-server",
    "test": "cross-env NODE_ENV=production npm run clean && webpack --config config/webpack.config.test.js",
    "pub": "cross-env NODE_ENV=production webpack --config config/webpack.config.prod.js",
    "clean": "rimraf dist",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.(js|jsx)": [
      "eslint --fix",
      "prettier --config .prettierrc --write",
      "git add"
    ],
    "src/**/*.scss": [
      "stylelint --syntax=scss --fix",
      "prettier --config .prettierrc --write",
      "git add"
    ],
    "src/**/*.css": [
      "stylelint --fix",
      "prettier --config .prettierrc --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.49",
    "@babel/core": "^7.0.0-beta.49",
    "@babel/node": "^7.0.0-beta.49",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.49",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0-beta.49",
    "@babel/plugin-transform-react-jsx": "^7.0.0-beta.49",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.49",
    "@babel/preset-env": "^7.0.0-beta.49",
    "autoprefixer": "^7.2.4",
    "babel-eslint": "^8.2.3",
    "babel-loader": "8.0.0-beta.0",
    "babel-plugin-transform-es3-member-expression-literals": "^6.22.0",
    "babel-plugin-transform-es3-property-literals": "^6.22.0",
    "cross-env": "^5.1.6",
    "css-loader": "^0.28.8",
    "eslint": "^4.16.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-config-standard-jsx": "^4.0.2",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-node": "^6.0.0",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-standard": "^3.1.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "html-beautify-webpack-plugin": "^1.0.5",
    "html-webpack-plugin": "^3.0.4",
    "husky": "^0.14.3",
    "json-loader": "~0.5.7",
    "lint-staged": "^7.1.2",
    "node-sass": "^4.9.0",
    "postcss-loader": "^2.1.5",
    "prettier": "^1.13.0",
    "raw-loader": "~0.5.1",
    "rimraf": "~2.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.1",
    "stylelint": "^9.2.1",
    "stylelint-config-prettier": "^2.1.0",
    "stylelint-config-standard": "^18.1.0",
    "ts-loader": "^3.3.1",
    "tslint": "~5.8.0",
    "tslint-config-prettier": "^1.13.0",
    "tslint-config-standard": "^7.0.0",
    "tslint-loader": "~3.5.3",
    "tslint-react": "^3.6.0",
    "typescript": "~2.6.2",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "url-loader": "^0.6.2",
    "webpack": "~3.10.0",
    "webpack-dev-middleware": "^2.0.4",
    "webpack-dev-server": "~2.10.0",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0-beta.49",
    "console-polyfill": "^0.3.0",
    "copy-webpack-plugin": "^4.5.1",
    "es5-polyfill": "0.0.6",
    "es6-object-assign": "^1.1.0",
    "es6-promise": "^4.1.1",
    "moment": "^2.22.1"
  }
}
